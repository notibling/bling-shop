{
  "name": "blinguy-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-icons-definitions": "cd src/icons/tsx && node __generate__icons_definitions.js",
    "dev": "npm run generate-icons-definitions && next dev",
    "dev-turbo": "npm run generate-icons-definitions && next dev --turbo",
    "dev-test": "concurrently \"npx tailwindcss -i ./src/app/globals.raw.css -o ./src/app/globals.css --watch\" \"next dev\" \"next lint --fix --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile-icons": "npx @svgr/cli --typescript --out-dir src/icons/tsx -- src/icons/raw ",
    "compile-icons-v2": "npx @svgr/cli --typescript --icon --expand-props end --replace-attr-values \"#000={props.color || 'currentColor'}\" --svg-props \"width={props.width || 24},height={props.height || 24}\" src/icons/raw --out-dir src/icons/tsx"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@mui/base": "^5.0.0-beta.40",
    "axios": "^1.6.8",
    "chart.js": "^4.4.3",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "next": "^14.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.5",
    "react-icons": "^5.1.0",
    "react-photo-view": "^1.2.6",
    "react-to-webcomponent": "^2.0.0",
    "swiper": "^11.1.1",
    "tailwind-merge": "^2.3.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@eslint/js": "^9.12.0",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "daisyui": "^4.10.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^0.2.4",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.10.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.2.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.8.0"
  }
}
