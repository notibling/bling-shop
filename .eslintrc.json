{
  "extends": "next",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "react/jsx-key": "off",
    "react/display-name": "off",
    "react-hooks/rules-of-hooks": "off",
    "@next/next/no-img-element": "off",
    "linebreak-style": ["error", "unix"]
  }
}
